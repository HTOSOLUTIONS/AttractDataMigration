@{
}
@model SQLStatementViewModel

<div class="snapshot">

    <form id="targettablecommitsql" asp-action="CommitSQLStatement" asp-route-tableschema="@Model.Table.TableSchema" asp-route-tablename="@Model.Table.TableName">
        <div class="form-group">
            <input type="submit" value="Commit" class="btn btn-primary" />
        </div>

    </form>
    <div spellcheck="false" class="sqlstatement" style="white-space: pre-wrap">
        @Html.Raw(Model.SQLStatement)
    </div>


</div>