@model List<ColumnSourceViewModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

@{

    string disp = "";
    if (Model != null && Model.Count > 0)
    {
        var list = Model.Select(c => new { source = c.SourceTable + '.' + c.SourceColumn }).ToList();
        disp = String.Join(",", list.Select(c => c.source));
    }

}

@Html.Raw(disp)