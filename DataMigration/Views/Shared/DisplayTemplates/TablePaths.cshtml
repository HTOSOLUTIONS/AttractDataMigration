@model string

@{

    var displayString = "";
    var links = new List<string>();
    var pathparts = Model.Split("~");
    @foreach (var part in pathparts)
    {
        links.Add("<a href=javascript:void(0) class='pathpart'>" + part + "</a>");
    }

    displayString = string.Join("~",links);


}

@Html.Raw(displayString)


